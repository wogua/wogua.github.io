            "1": {
                "align": "{{ {enum.GridLayout.Align.FillWidth} | {enum.GridLayout.Align.Top} }}",
                "margin": {"top": "{dp(3)}"}
            },
            "2": {
                "align": "{{ {enum.GridLayout.Align.FillWidth} | {enum.GridLayout.Align.Top} }}",
                "margin": {"top": "{dp(3)}"}
            },
            "3": {
                "align": "{{ {enum.GridLayout.Align.FillWidth} | {enum.GridLayout.Align.Top} }}",
                "margin": {"top": "{dp(3)}"}
            }
        }
    }
}
